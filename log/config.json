{
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
        "simple": {
            "format": "[%(levelname)s|%(module)s|L%(lineno)d] %(asctime)s: %(message)s",
            "datefmt": "%Y-%m-%dT%H:%M:%S%z"
        },
        "simple2": {
            "format": "[%(levelname)s|%(module)s|] %(asctime)s: %(message)s",
            "datefmt": "%Y-%m-%dT%H:%M:%S%z"
        },
        "json": {
            "()": "log.logging_config.MyJSONFormatter",
            "fmt_keys": {
                "level": "levelname",
                "message": "message",
                "timestamp": "timestamp",
                "logger": "name",
                "module": "module",
                "function": "funcName",
                "line": "lineno",
                "thread_name": "threadName"
            }
        }
    },
    "filters": {
        "unwanted_logs": {
            "()": "log.logging_config.ExcludeFilter",
            "names": [
                "httpcore",
                "hpack",
                "asyncio"
            ]
        }
    },
    "handlers": {
        "stdout": {
            "class": "logging.StreamHandler",
            "level": "INFO",
            "formatter": "simple2",
            "stream": "ext://sys.stdout"
        },
        "stderr": {
            "class": "logging.StreamHandler",
            "level": "WARNING",
            "formatter": "simple",
            "stream": "ext://sys.stderr"
        },
        "file_json": {
            "class": "logging.handlers.RotatingFileHandler",
            "level": "DEBUG",
            "formatter": "json",
            "filename": "",
            "maxBytes": 5000000,
            "backupCount": 5,
            "filters": [
                "unwanted_logs"
            ]
        },
        "queue_handler": {
            "class": "logging.handlers.QueueHandler",
            "handlers": [
                "stdout",
                "stderr",
                "file_json"
            ],
            "respect_handler_level": true
        }
    },
    "loggers": {
        "root": {
            "level": "DEBUG",
            "handlers": [
                "queue_handler"
            ]
        },
        "httpx": {
            "handlers": [
                "file_json"
            ],
            "level": "WARNING",
            "propagate": false
        },
        "httpcore.http2": {
            "level": "WARNING",
            "handlers": [
                "file_json"
            ],
            "propagate": false
        }
    }
}